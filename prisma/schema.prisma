
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Instagram {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  text     String
  postUrl  String
  imageSrc String
  videoSrc String
  isVideo  Boolean?
}

model LinkedIn {
  id       String   @id @default(auto()) @map("_id") @db.ObjectId
  text     String
  postUrl  String
  imageSrc String
  videoSrc String
  article  String
}
